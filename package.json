{
    "name": "mongodb-erd-cli",
    "version": "2.0.0",
    "description": "CLI tool to generate Entity-Relationship Diagrams (ERD) from MongoDB databases",
    "main": "src/index.js",
    "type": "module",
    "bin": {
        "mongodb-erd": "./bin/mongodb-erd.js"
    },
    "scripts": {
        "test": "NODE_OPTIONS=--experimental-vm-modules jest",
        "lint": "eslint .",
        "version": "node scripts/version.js",
        "release:patch": "npm version patch",
        "release:minor": "npm version minor",
        "release:major": "npm version major",
        "release:beta": "npm version prerelease --preid=beta",
        "release:alpha": "npm version prerelease --preid=alpha"
    },
    "keywords": [
        "mongodb",
        "erd",
        "diagram",
        "mermaid",
        "cli"
    ],
    "author": "Michael Lynn",
    "license": "MIT",
    "dependencies": {
        "commander": "^11.1.0",
        "inquirer": "^9.2.14",
        "mongodb": "^6.3.0",
        "puppeteer": "^22.3.0"
    },
    "devDependencies": {
        "@babel/core": "^7.24.0",
        "@babel/preset-env": "^7.24.0",
        "@jest/globals": "^29.7.0",
        "eslint": "^8.57.0",
        "jest": "^29.7.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mlynn/mongodb-erd-cli.git"
    },
    "bugs": {
        "url": "https://github.com/mlynn/mongodb-erd-cli/issues"
    },
    "homepage": "https://github.com/mlynn/mongodb-erd-cli#readme"
}
