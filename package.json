{
    "name": "mongodb-erd-cli",
    "version": "2.5.1",
    "description": "A CLI tool to generate Entity Relationship Diagrams (ERD) from MongoDB databases",
    "main": "src/index.js",
    "type": "module",
    "bin": {
        "mongodb-erd": "./bin/mongodb-erd.js"
    },
    "scripts": {
        "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
        "lint": "eslint .",
        "version": "node scripts/version.js",
        "release:patch": "npm version patch",
        "release:minor": "npm version minor",
        "release:major": "npm version major",
        "release:beta": "npm version prerelease --preid=beta",
        "release:alpha": "npm version prerelease --preid=alpha"
    },
    "keywords": [
        "mongodb",
        "erd",
        "cli",
        "diagram",
        "entity-relationship",
        "database",
        "visualization",
        "mermaid",
        "ascii"
    ],
    "author": "Michael Lynn",
    "license": "MIT",
    "dependencies": {
        "commander": "^11.1.0",
        "dotenv": "^16.4.5",
        "inquirer": "^9.2.14",
        "mongodb": "^6.3.0",
        "mermaid": "^10.8.0",
        "puppeteer": "^22.3.0",
        "boxen": "^7.1.1"
    },
    "devDependencies": {
        "eslint": "^8.57.0",
        "jest": "^29.7.0",
        "prettier": "^3.2.5"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mrlynn/mongodb-erd.git"
    },
    "bugs": {
        "url": "https://github.com/mrlynn/mongodb-erd/issues"
    },
    "homepage": "https://github.com/mrlynn/mongodb-erd#readme"
}
